{
  "name": "astro",
  "type": "module",
  "version": "0.2.0",
  "scripts": {
    "dev": "astro dev",
    "lint": "biome lint .",
    "lint:md": "markdownlint-cli2",
    "lint:md:fix": "markdownlint-cli2 --fix",
    "style:lint": "pnpm run stylelint './src/**/*.css' './**/snippets/CSS/**/*.css' --cache",
    "style:lint:fix": "pnpm run style:lint --fix",
    "lint:fix": "biome lint . --write",
    "format": "biome format . --write",
    "start": "astro dev",
    "build": "astro build",
    "preview": "astro preview",
    "astro": "astro",
    "typecheck": "tsc --noEmit",
    "check": "biome check --write .",
    "spell:check": "cspell lint --no-progress --show-context --relative ."
  },
  "dependencies": {
    "@astrojs/sitemap": "^3.6.1",
    "@astrojs/starlight": "^0.37.3",
    "@catppuccin/starlight": "^1.0.2",
    "astro": "^5.16.8",
    "astro-embed": "^0.12.0",
    "astro-og-canvas": "^0.10.0",
    "canvaskit-wasm": "^0.40.0",
    "sharp": "^0.34.5",
    "starlight-blog": "^0.25.2",
    "starlight-kbd": "^0.3.0",
    "starlight-links-validator": "^0.19.2",
    "starlight-package-managers": "^0.12.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.2.5",
    "markdownlint-cli2": "^0.20.0",
    "markdownlint-cli2-formatter-pretty": "^0.0.9",
    "stylelint": "^16.2.0",
    "stylelint-config-standard": "^37.0.0",
    "stylelint-order": "^6.0.4",
    "stylelint-plugin-defensive-css": "^1.0.2",
    "stylelint-use-logical-spec": "^5.0.1",
    "typescript": "^5.9.3",
    "cspell": "^8.14.3"
  },
  "packageManager": "pnpm@10.17.1"
}
